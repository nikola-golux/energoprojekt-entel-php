php_flag display_errors on
php_value error_reporting 9999

RewriteEngine On

# RewriteRulz
RewriteRule ^history/?$ index.php [NC,L] #Istorija
RewriteRule ^employees/?$ employees.php [NC,L] #Zaposleni
RewriteRule ^firms/?$ firms.php [NC,L] #Firme
RewriteRule ^new-employee/?$ new_employee.php [NC,L] #Novi Zaposleni
RewriteRule ^new-firm/?$ new_firm.php [NC,L] #Nova Firma